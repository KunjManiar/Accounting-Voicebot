<div class="fabs">
    <div class="chat">
        <div class="chat_header">
            <div class="chat_option">
                <div class="header_img">
                    <img src="{{ url_for('static',filename='img/logo2.jpg')}}"/>
                </div>
                <span id="chat_head">Eazy</span> <br> <span class="agent">Agent</span> <span
                    class="online">(Online)</span>
                <span id="chat_fullscreen_loader" class="chat_fullscreen_loader"><i
                        class="fullscreen zmdi zmdi-window-maximize"></i></span>

            </div>

        </div>

        <form id="voiceForm" method="post" action="{{ url_for('voicebot.index') }}">
        <div class="fab_field" style="padding-bottom: 5px;">
            <!--<span id="chatSend" name="chat_message" class="chat_field chat_message"></span>-->
            <span style="display: none" id="chatSendfinal" name="chat_message1" class="chat_field chat_message"></span>

              <img onclick="startButton(event)" id="fab_camera" class="fab" src="{{ url_for('static',filename='img/mic.gif')}}" alt="Start" style="margin: 49px 0px 49px 0px;">
            <textarea id="chatSend" name="data"  class="chat_field chat_message" rows="4" cols=""20 type="text" value="" style="width: 71%"></textarea>

            <a id="fab_send" class="fab" style="margin: 57px 2px 40px 4px;" onclick="document.getElementById('voiceForm').submit()"><i style="font-size: 2.1em" class="zmdi zmdi-mail-send"></i></a>
        </div>
        <div clas="fab_field">
              <!--<a id="fab_camera" class="fab"><i class="zmdi zmdi-camera"></i></a>-->
{#            <a id="fab_send" class="fab"><i class="zmdi zmdi-mail-send"></i></a>#}
{#              <img onclick="startButton(event)" id="fab_camera" class="fab" src="{{ url_for('static',filename='img/mic.gif')}}" alt="Start">#}
        </div>

        </form>
    </div>
    <a id="prime" class="fab"><i class="prime zmdi zmdi-comment-outline"></i></a>
</div>